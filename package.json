{
  "name": "fluui",
  "version": "1.1.12",
  "description": "simple ui for media",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run svg && npm run compile && npm run style",
    "compile": "BABEL_ENV=production node_modules/.bin/babel --out-dir='lib' --ignore='__tests__/*' src",
    "style": "node scripts/style.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "svg": "node scripts/svg.js",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --print-width 120 --no-semi --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/369857519/Flu-UI.git"
  },
  "keywords": [
    "react",
    "icon",
    "UI"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/369857519/Flu-UI/issues"
  },
  "homepage": "https://github.com/369857519/Flu-UI#readme",
  "peerDependencies": {
    "react": ">=15.4.0",
    "react-dom": ">=15.4.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.6",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^7.2.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.5",
    "eslint": "^4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.0",
    "file": "^0.2.2",
    "fs-extra": "^4.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "node-sass": "^4.5.3",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "rc-dialog": "^6.5.10",
    "rc-notification": "^2.0.5",
    "rc-util": "^4.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-select": "^1.0.0-rc.5",
    "react-svg-loader": "^1.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2"
  }
}
